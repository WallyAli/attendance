<h2>Child's detail information</h2>

<%= @child.first_name %> <%= @child.last_name %></br>

<ul>
<% @child.schedules.each do |schedule| %>
  <li><%= schedule.date %></li>
  <li><%= schedule.time_in %></li>
  <li><%= schedule.time_out %></li>
<% end %></br>

<%= link_to 'create schedule', new_child_schedule_path(
@child) %>
